{
    "buildCommand": "cd food && npm install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "food/dist",
    "framework": "vite",
    "rewrites": [ 
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      },
      {"source": "/(.*)", "destination": "/"}
    ]
  }
